{
  "expo": {
    "name": "Datos",
    "slug": "Datos",
    "version": "6.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.datos.app",
    "userInterfaceStyle": "automatic",
    "notification": {
      "icon": "./assets/images/icon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6cc3502e-dafa-474f-bea0-051818b5653a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.datos.app",
      "buildNumber": "6.0",
      "usesAppleSignIn": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSCameraUsageDescription": "We need to use your camera to be able to set your profile picture, scan QR Codes and upload requirements",
        "NSPhotoLibraryUsageDescription": "We need to access your photos to be able to set your profile picture and upload requirements"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.datos.app",
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "NOTIFICATIONS",
        "MEDIA_LIBRARY",
        "READ_INTERNAL_STORAGE",
        "WRITE_INTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "blockedPermissions": [
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ],
      "versionCode": 60,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6cc3502e-dafa-474f-bea0-051818b5653a"
      }
    },
    "plugins": [],
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
